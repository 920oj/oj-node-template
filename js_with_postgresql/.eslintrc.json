{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "camelcase": 0,
        "prefer-destructuring": 0,
        "no-param-reassign": 0,
        "no-await-in-loop": 0,
        "no-async-promise-executor": 0
    }
}